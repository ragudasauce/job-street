<%= form_for @location do |f| %>
	<p><%= f.label :street %> : <%= f.text_field :street %></p>
	<p><%= f.label :city %> : <%= f.text_field :city %></p>
	<p><%= f.label :state %> : <%= f.text_field :state %></p>
	<p><%= f.label :country %> : <%= f.text_field :country %></p>
	<p><%= f.label :zip %> : <%= f.text_field :zip %></p>
	<p><%= f.label :latitude %> : <%= f.text_field :latitude %></p>
	<p><%= f.label :longitude %> : <%= f.text_field :longitude %></p>
	<p><%= f.submit %></p>
<% end %>