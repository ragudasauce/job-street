<h1>Sign up</h1>
<p>Tell us a little about yourself. We'll tell you a lot about your next job.</p>

<%= form_for @user do |user_form| %>
	<p><%= user_form.label :first_name, "First name" %>: <%= user_form.text_field :first_name %></p>
	<p><%= user_form.label :last_name, "Last name"%>: <%= user_form.text_field :last_name %></p>
	<p><%= user_form.label :email, "email" %>: <%= user_form.text_field :email %></p>
	<p><%= user_form.label :password, "password" %>: <%= user_form.text_field :password %></p>
	<p><%= user_form.submit %></p>
<% end %>